  "tags": [
    {"name": "Prices", "description": "Real-time and historical stock prices"},
    {"name": "Financials", "description": "Income statements, balance sheets, cash flow"},
    {"name": "Insider Trading", "description": "SEC Form 4 insider transactions"},
    {"name": "Institutional", "description": "13F institutional ownership data"},
    {"name": "Analysts", "description": "Analyst ratings and estimates"},
