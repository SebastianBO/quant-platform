    "version": "1.0.0",
    "contact": {
      "name": "Lician API Support",
      "email": "api@lician.com",
      "url": "https://lician.com/developers"
    },
    "license": {
      "name": "API Terms of Service",
      "url": "https://lician.com/terms"
    },
    "x-logo": {
      "url": "https://lician.com/logo.png"
    }
  },
  "servers": [
    {
      "url": "https://lician.com",
      "description": "Production server"
    }
  ],
  "externalDocs": {
    "description": "Full API Documentation",
    "url": "https://lician.com/api-docs"
