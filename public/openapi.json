    "/api/v1/insider-trades": {
      "get": {
        "operationId": "getInsiderTrades",
        "summary": "Get insider trading activity",
        "description": "Returns SEC Form 4 insider transactions including buys, sells, and option exercises",
        "tags": ["Insider Trading"],
        "parameters": [
          {
            "name": "ticker",
            "in": "query",
            "required": false,
            "schema": {"type": "string"},
            "description": "Filter by stock ticker (optional, returns all if omitted)",
            "example": "AAPL"
          },
          {
            "name": "limit",
            "in": "query",
            "required": false,
            "schema": {"type": "integer", "default": 50, "maximum": 500},
            "description": "Number of trades to return"
          },
          {
            "name": "transaction_type",
            "in": "query",
            "required": false,
            "schema": {"type": "string", "enum": ["P-Purchase", "S-Sale", "A-Award", "M-Exercise"]},
            "description": "Filter by transaction type"
          }
        ],
        "responses": {
          "200": {
            "description": "Successful response",
            "content": {
              "application/json": {
                "schema": {"$ref": "#/components/schemas/InsiderTrades"},
                "example": {
                  "insider_trades": [{
                    "ticker": "AAPL",
                    "insider_name": "Tim Cook",
                    "insider_title": "CEO",
                    "transaction_type": "S-Sale",
                    "shares": 50000,
                    "price": 185.50,
                    "value": 9275000,
                    "shares_owned_after": 3200000,
                    "filing_date": "2024-12-15",
                    "transaction_date": "2024-12-13"
                  }]
                }
              }
            }
          }
        }
      }
    },
