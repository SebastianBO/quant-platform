  "components": {
    "schemas": {
      "PriceSnapshot": {
        "type": "object",
        "properties": {
          "snapshot": {
            "type": "object",
            "properties": {
              "ticker": {"type": "string"},
              "price": {"type": "number"},
              "day_change": {"type": "number"},
              "day_change_percent": {"type": "number"},
              "volume": {"type": "integer"},
              "market_cap": {"type": "integer"},
              "open": {"type": "number"},
              "high": {"type": "number"},
              "low": {"type": "number"},
              "previous_close": {"type": "number"}
            }
          }
        }
      },
      "HistoricalPrices": {
        "type": "object",
        "properties": {
          "prices": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "date": {"type": "string", "format": "date"},
                "open": {"type": "number"},
                "high": {"type": "number"},
                "low": {"type": "number"},
                "close": {"type": "number"},
                "volume": {"type": "integer"},
                "adjusted_close": {"type": "number"}
              }
            }
          }
        }
      },
      "IncomeStatements": {
        "type": "object",
        "properties": {
          "income_statements": {
            "type": "array",
            "items": {
              "type": "object",
              "properties": {
                "ticker": {"type": "string"},
                "report_period": {"type": "string", "format": "date"},
                "period": {"type": "string", "enum": ["quarterly", "annual"]},
                "revenue": {"type": "integer"},
                "cost_of_revenue": {"type": "integer"},
                "gross_profit": {"type": "integer"},
                "operating_expenses": {"type": "integer"},
                "operating_income": {"type": "integer"},
                "net_income": {"type": "integer"},
                "eps": {"type": "number"},
                "eps_diluted": {"type": "number"},
                "shares_outstanding": {"type": "integer"}
              }
            }
          }
