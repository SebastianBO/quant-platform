{
  "installCommand": "npm install --legacy-peer-deps",
  "env": {
    "VERCEL_BYPASS_FALLBACK_OVERSIZED_ERROR": "1"
  },
  "crons": [
    {
      "path": "/api/cron/sync-portfolios",
      "schedule": "0 21 * * 1-5"
    },
    {
      "path": "/api/cron/sync-financials?mode=continue&limit=5",
      "schedule": "0 6 * * *"
    }
  ]
}
